package com.qualcomm.ftcrobotcontroller.opmodes;
import ...

public class ArmControl extends OpModede {
  DcMotor leftArm;
  
  @Override
  public void init(){
    leftArm = hardwareMap.dcMotor.get("left_arm");
  }
  
  @Override
  public void loop(){
  
    if(gamepad1.y){
      leftArm.setPower(0.1);
    }
    
    else if(gamepad1.b){
      leftArm.setPower(-0.1);
    }
    
    else{
      leftArm.setPower(0);
    }
  
  }
}
